	<parameter name="order_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select @rownum:=ifnull(@rownum,0)+1 as seq_no, costCheckOrder.*,ifnull(ul.c_name,ul.user_name) as create_name,ifnull(u.c_name,u.user_name) as confirm_name from
	(select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
		dor.order_no as order_no,
		dor.appointment_stamp as create_date,
		'' as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'配送' as service_type,
		(select sum(amount) from delivery_order_item doit where doit.delivery_id = dor.id) as quantity,
		IFNULL(
		dor.receivingunit,
		IFNULL(
			trid.notify_party_company,
			''
		)
	) as address,
		ifnull((select sum(amount) from delivery_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.order_id = dor.id and fi.type = '应付' and ifnull(dofi.cost_source,'')!='对账调整金额'),0) as amount,
		ifnull((select sum(amount) from delivery_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.order_id = dor.id and fi.type = '应付'),0) as confirm_amount,
		dor.remark as remark
	from
		arap_cost_item aci
	left join delivery_order dor on dor.id = aci.ref_order_id
  LEFT JOIN delivery_order_item dt2 ON dt2.delivery_id = dor.id
  LEFT JOIN transfer_order_item_detail trid ON trid.id = dt2.transfer_item_detail_id
	left join party p on dor.customer_id = p.id
	left join contact c on c.id = p.contact_id
	left join location r_f on r_f.`code` = dor.route_from
	left join location r_t on r_t.`code` = dor.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='配送'
union
 select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
		dto.depart_no  as order_no,
	  dto.departure_time as create_date,
		dto.booking_note_number as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'零担' as service_type,
		(SELECT CASE WHEN t.cargo_nature = 'ATM' THEN (SELECT count(toid.id) FROM transfer_order_item_detail toid WHERE toid.depart_id = dto.id )
		WHEN t.cargo_nature = 'cargo' THEN ( SELECT sum(toi.amount) FROM depart_order dpr2 LEFT JOIN depart_transfer dt ON dt.depart_id = dpr2.id LEFT JOIN transfer_order_item toi ON toi.order_id = dt.order_id
		WHERE dpr2.id = dto.id )END) as quantity,
		(
SELECT group_concat(DISTINCT c.address SEPARATOR '\r\n')
from contact c
left join transfer_order t on t.notify_party_id=c.id
LEFT JOIN depart_transfer dt on t.id = dt.order_id
left join depart_order dto on dto.id =dt.depart_id
where dto.id = aci.ref_order_id
 ) as address,
		ifnull((select sum(amount) from depart_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.depart_order_id = dto.id and fi.type = '应付' and ifnull(dofi.cost_source,'')!='对账调整金额'),0) as amount,
		ifnull((select sum(amount) from depart_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.depart_order_id = dto.id and fi.type = '应付'),0) as confirm_amount,
		dto.remark as remark
	from
		arap_cost_item aci
	left join depart_order dto on dto.id = aci.ref_order_id
	left join depart_transfer dt on dt.depart_id = dto.id
	left join transfer_order t on t.id = dt.order_id
            LEFT JOIN warehouse w on t.warehouse_id=w.id
            left join location lc on lc.code = w.location
	left join party p on t.customer_id = p.id
	left join contact c on p.contact_id = c.id
	left join location r_f on r_f.`code` = t.route_from
	left join location r_t on r_t.`code` = t.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='零担'
GROUP BY dto.id
union
	select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
	  doer.depart_no as order_no,
		doer.create_stamp   as create_date,
		doer.booking_note_number as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'提货' as service_type,
		(SELECT
sum(amount)
FROM
depart_transfer
WHERE
pickup_id = doer.id) as quantity,
		doer.address as address,
		ifnull((select sum(amount) from pickup_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.pickup_order_id = doer.id and fi.type = '应付' and ifnull(dofi.cost_source,'')!='对账调整金额'),0) as amount,
		ifnull((select sum(amount) from pickup_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.pickup_order_id = doer.id and fi.type = '应付'),0) as confirm_amount,
		doer.remark as remark
	from
		arap_cost_item aci
	left join depart_order doer on doer.id = aci.ref_order_id
	left join depart_transfer dt on dt.pickup_id  = doer.id
	left join transfer_order t on t.id = dt.order_id
	left join party p on t.customer_id = p.id
	left join contact c on p.contact_id = c.id
	left join location r_f on r_f.`code` = t.route_from
	left join location r_t on r_t.`code` = t.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='提货'
group by doer.id
union
	select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
	  io.order_no  as order_no,
		io.create_stamp as create_date,
		'' as sp_order_no,
		( SELECT   group_concat(DISTINCT c.abbr SEPARATOR '\r\n') from contact c
left join party p on p.contact_id = c.id
left join transfer_order tor on tor.customer_id = p.id
where tor.insurance_id=io.id)  customer_name,
		'' as route_from,
		'' as route_to,
		'保险' as service_type,
		(select sum(amount) from transfer_order_item where order_id in (select id from transfer_order where insurance_id = io.id)) as quantity,
		'' as address,
		ifnull((select sum(insurance_amount) from insurance_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.insurance_order_id = io.id and fi.type = '应付' and ifnull(dofi.cost_source,'')!='对账调整金额'),0) as amount,
		ifnull((select sum(insurance_amount) from insurance_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.insurance_order_id = io.id and fi.type = '应付'),0) as confirm_amount,
		io.remark as remark
	from
		arap_cost_item aci
	left join  insurance_order io on io.id = aci.ref_order_id
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='保险'
union
SELECT
            aci.ref_order_no,
            aci.ref_order_id,
            aci.cost_order_id,
	'' AS order_no,
	amco.create_stamp AS create_date,
	'' AS sp_order_no,
	c.abbr AS customer_name,
	r_f.`name` AS route_from,
	r_t.`name` AS route_to,
	'成本单' AS service_type,
	0 AS quantity,
	'' AS address,
	ifnull((SELECT sum(amount) FROM arap_misc_cost_order_item amcoi left join fin_item fi on  fi.id = amcoi.fin_item_id where amcoi.misc_order_id = amco.id and fi.type = '应付'),0) AS amount,
	ifnull((SELECT sum(amount) FROM arap_misc_cost_order_item amcoi left join fin_item fi on fi.id = amcoi.fin_item_id where amcoi.misc_order_id = amco.id and fi.type = '应付'),0) AS confirm_amount,
	amco.remark AS remark
       FROM
	arap_cost_item aci
	LEFT JOIN arap_misc_cost_order amco ON amco.id = aci.ref_order_id
	LEFT JOIN arap_cost_order aco ON aco.id = aci.cost_order_id
	LEFT JOIN party p ON amco.customer_id = p.id
	LEFT JOIN contact c ON p.contact_id = c.id
	LEFT JOIN location r_f ON r_f.`code` = amco.route_from
	LEFT JOIN location r_t ON r_t.`code` = amco.route_to
	WHERE
	aci.cost_order_id = $P{order_id}
            AND aci.ref_order_no = '成本单'

)as costCheckOrder left join arap_cost_order acor on acor.id = costCheckOrder.cost_order_id
	left join user_login ul on  ul.id = acor.create_by left join  user_login u on  u.id = acor.confirm_by]]>
	</queryString>
