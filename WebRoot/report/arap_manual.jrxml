	<queryString>
		<![CDATA[SELECT
amco.id order_id,
amco.order_no oder_no,
ifnull(amco.remark,'') remark,
ul.c_name creator_name,
	IFNULL(
		(
			CASE amco.charge_from_type
			WHEN 'customer' THEN
				c1.company_name
			WHEN 'sp' THEN
				c.company_name
			ELSE
				amco.others_name
			END
		),
		''
	) AS chargr_name,
	IFNULL(o.abbr, '') office_name,
	IFNULL(amco.create_stamp, '') create_stamp
FROM
	arap_misc_charge_order amco
LEFT JOIN party p ON p.id = amco.sp_id and p.party_type='SERVICE_PROVIDER'
LEFT JOIN contact c on c.id =p.contact_id
LEFT JOIN party p1 ON p1.id = amco.customer_id and p1.party_type='CUSTOMER'
LEFT JOIN contact c1 on c1.id =p1.contact_id
LEFT JOIN office o ON o.id = amco.office_id
LEFT JOIN user_login ul ON ul.id = amco.create_by
where amco.order_no =$P{order_no}]]>
	</queryString>
