	<queryString>
		<![CDATA[SELECT
     damage_order_fin_item.`id` AS id,
     damage_order_fin_item.`order_id` AS order_id,
     damage_order_fin_item.`type` AS type,
     damage_order_fin_item.`status` AS status,
     ifnull(damage_order_fin_item.`fin_item`, '')  AS fin_item,
     damage_order_fin_item.`amount` AS amount,
     damage_order_fin_item.`party_type` AS party_type,
     damage_order_fin_item.`party_name` AS party_name,
     damage_order_fin_item.`fin_method` AS fin_method,
     ifnull(damage_order_fin_item.`remark`, '')  AS remark
FROM
     `damage_order_fin_item` damage_order_fin_item
