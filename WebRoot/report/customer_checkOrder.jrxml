	<queryString>
		<![CDATA[SELECT DISTINCT
	aao.id,
	ifnull(aao.order_no,'') order_no,
	ifnull(aao.begin_time,'') begin_time,
            MONTH (aao.begin_time) c_stamp,
	ifnull(aao.billing_unit,c.company_name) cname,
            ifnull(o.office_name,'') office_name,
	ifnull(aao.remark,'') remark,
	usl.c_name create_name
FROM
	arap_charge_order aao
LEFT JOIN party p ON p.id = aao.payee_id
LEFT JOIN contact c ON c.id = p.contact_id
LEFT JOIN contact c1 ON c1.id = aao.sp_id
LEFT JOIN user_login usl ON usl.id = aao.create_by
LEFT JOIN office o ON o.id = usl.office_id
where aao.order_no=$P{order_no}]]>
	</queryString>
