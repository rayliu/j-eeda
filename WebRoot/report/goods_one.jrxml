	<parameter name="order_no" class="java.lang.String"/>
	<queryString>
		<![CDATA[select tor.id,tor.order_no ,
	o.office_name,
	ifnull(ul.c_name,ul.user_name) user_name,
	concat(l1.name, l.`name`) as route_from,
	concat(rt.name , r.`name`) as route_to,
	tor.arrival_time,
	tor.receiving_unit,
	tor.address,
	c.contact_person,
	c.phone,
	c.address as s_address
 from transfer_order tor
left join user_login ul on ul.id = tor.create_by
left join office o on ul.office_id = o.id
left join location l on tor.route_from = l.code
left join location l1 on l.pcode = l1.code
left join location r on tor.route_to = r.code
left join location rt on r.pcode = rt.code
left join party p on tor.notify_party_id = p.id
left join contact c on p.contact_id = c.id
where order_no = $P{order_no}]]>
	</queryString>
