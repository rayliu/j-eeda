	<parameter name="order_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
r_o.order_no,
	 ifnull(
		tor.customer_order_no,
		tor2.customer_order_no
	) customer_order_no,
	ifnull(lo2. NAME,'') to_name,
IFNULL(tor.planning_time,'')  planning_time,
ifnull(
						c4.address,
						(
							SELECT
								c.address
							FROM
								contact c
							LEFT JOIN party p ON p.id = c.id
							WHERE
								p.id = tor.notify_party_id
						)
					) receipt_address,
            (
							SELECT group_concat(DISTINCT ifnull(p.item_name, ifnull(toi.item_name,'')) SEPARATOR '')
							FROM
								transfer_order_item toi
							LEFT JOIN product p ON p.id = toi.product_id
							WHERE
								toi.order_id =tor.id
						) item_name,
       ((
				SELECT
					IFNULL(sum(doi1.amount),0)
				FROM
					delivery_order_item doi1
				WHERE
					doi1.delivery_id = d_o.id
			) +
(
				SELECT
					IFNULL(sum(toi.amount),0)
				FROM
					transfer_order_item toi
				WHERE
					toi.order_id = tor.id
			))amount,
       (SELECT
					ROUND(IFNULL(sum(toi.volume),0),2)
				FROM
					transfer_order_item toi
				WHERE
					toi.order_id = tor.id) volume,
    (SELECT
					ROUND(IFNULL(sum(toi.weight),0),2)
				FROM
					transfer_order_item toi
				WHERE
					toi.order_id = tor.id) weight,
(
				SELECT
					IFNULL(sum(rofi.amount),0)
				FROM
					return_order_fin_item rofi
				WHERE
					rofi.return_order_id = r_o.id
			) total_amount,
      (
				SELECT
					IFNULL(sum(rofi.change_amount),sum(rofi.amount))
				FROM
					return_order_fin_item rofi
				WHERE
					rofi.return_order_id = r_o.id
			) change_amount,
     r_o.remark,
ifnull(ul.c_name,ul.user_name) as create_name
FROM
	arap_charge_item aci
LEFT JOIN arap_charge_order aco on aco.id =aci.charge_order_id
LEFT JOIN return_order r_o ON r_o.id = aci.ref_order_id
LEFT JOIN delivery_order d_o ON d_o.id = r_o.delivery_order_id
LEFT JOIN delivery_order_item doi ON doi.delivery_id = d_o.id
LEFT JOIN transfer_order tor2 ON tor2.id = doi.transfer_order_id
LEFT JOIN transfer_order tor ON tor.id = r_o.transfer_order_id
LEFT JOIN location lo2 ON ifnull(tor.route_to, tor2.route_to) = lo2. CODE
LEFT JOIN party p4 ON p4.id = d_o.notify_party_id
LEFT JOIN contact c4 ON c4.id = p4.contact_id
left join user_login ul on ul.id = aco.create_by
where aci.charge_order_id=$P{order_id}]]>
	</queryString>
