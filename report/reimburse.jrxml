	<queryString>
		<![CDATA[select
ro.id id,
ro.order_no order_no,
ro.create_stamp create_stamp,
IFNULL(o.abbr,IFNULL(o.office_name,'')) office_name,
ro.payment_type payment_type,
ro.account_name account_name,
ro.account_bank account_bank,
ro.account_no account_no
from reimbursement_order ro
LEFT JOIN user_login ul on ul.id= ro.create_id
LEFT JOIN office o on o.id=ul.office_id
where ro.order_no like 'YFBX%' and ro.order_no=$P{order_no}]]>
	</queryString>
