	<queryString>
		<![CDATA[SELECT
    aci.ref_order_no as service_type,
    aco.order_no as order_no,
    aco.create_stamp as create_stamp,
    aco.confirm_stamp as confirm_stamp,
    aco.total_amount as total_amount,
    aco.cost_amount as cost_amount,
    (aco.total_amount - aco.cost_amount) as cost_balance
    from
    arap_cost_order aco
    left join arap_cost_item aci on aci.cost_order_id = aco.id
where aco.application_order_id = $P{order_id}]]>
	</queryString>
