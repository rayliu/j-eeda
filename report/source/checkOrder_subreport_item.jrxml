	<parameter name="order_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select @rownum:=ifnull(@rownum,0)+1 as seq_no, costCheckOrder.*,ifnull(ul.c_name,ul.user_name) as create_name,ifnull(u.c_name,u.user_name) as confirm_name from
	(select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
		dor.order_no as order_no,
		dor.Create_stamp as create_date,
		'' as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'配送' as service_type,
		(select count(*) from delivery_order_item doit where doit.delivery_id = dor.id) as quantity,
		'' as address,
		(select sum(amount) from delivery_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.order_id = dor.id and fi.type = '应付') as amount,
		'' as confirm_amount,
		'' as remark
	from
		arap_cost_item aci
	left join delivery_order dor on dor.id = aci.ref_order_id
	left join party p on dor.customer_id = p.id
	left join contact c on c.id = p.contact_id
	left join location r_f on r_f.`code` = dor.route_from
	left join location r_t on r_t.`code` = dor.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='配送'
union
 select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
		dto.depart_no  as order_no,
	  dto.create_stamp as create_date,
		dto.booking_note_number as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'零担' as service_type,
		(select sum(amount) from transfer_order_item where order_id in (select order_id from depart_transfer dtp where dtp.depart_id = dto.id )) as quantity,
		'' as address,
		(select sum(amount) from depart_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.depart_order_id = dto.id and fi.type = '应付') as amount,
		'' as confirm_amount,
		'' as remark
	from
		arap_cost_item aci
	left join depart_order dto on dto.id = aci.ref_order_id
	left join depart_transfer dt on dt.depart_id = dto.id
	left join transfer_order t on t.id = dt.order_id
	left join party p on t.customer_id = p.id
	left join contact c on p.contact_id = c.id
	left join location r_f on r_f.`code` = t.route_from
	left join location r_t on r_t.`code` = t.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='零担'
union
	select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
	  doer.depart_no as order_no,
		doer.create_stamp as create_date,
		doer.booking_note_number as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'提货' as service_type,
		(select sum(amount) from transfer_order_item where order_id in (select order_id from depart_transfer dtp where dtp.depart_id = doer.id )) as quantity,
		'' as address,
		(select sum(amount) from pickup_order_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.pickup_order_id = doer.id and fi.type = '应付') as amount,
		'' as confirm_amount,
		'' as remark
	from
		arap_cost_item aci
	left join depart_order doer on doer.id = aci.ref_order_id
	left join depart_transfer dt on dt.depart_id = doer.id
	left join transfer_order t on t.id = dt.order_id
	left join party p on t.customer_id = p.id
	left join contact c on p.contact_id = c.id
	left join location r_f on r_f.`code` = t.route_from
	left join location r_t on r_t.`code` = t.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='提货'
union
	select
		aci.ref_order_no,
		aci.ref_order_id,
		aci.cost_order_id,
	  io.order_no  as order_no,
		io.create_stamp as create_date,
		'' as sp_order_no,
		c.abbr as customer_name,
		r_f.`name` as route_from,
		r_t.`name` as route_to,
		'保险' as service_type,
		(select sum(amount) from transfer_order_item where order_id in (select id from transfer_order where insurance_id = io.id)) as quantity,
		'' as address,
		(select sum(insurance_amount) from insurance_fin_item dofi left join fin_item fi on fi.id = dofi.fin_item_id where dofi.insurance_order_id = io.id and fi.type = '应付') as amount,
		'' as confirm_amount,
		'' as remark
	from
		arap_cost_item aci
	left join  insurance_order io on io.id = aci.ref_order_id
	left join transfer_order tor on tor.insurance_id = io.id
	left join party p on tor.customer_id = p.id
	left join contact c on p.contact_id = c.id
	left join location r_f on r_f.`code` = tor.route_from
	left join location r_t on r_t.`code` = tor.route_to
	where
		aci.cost_order_id = $P{order_id}
		and aci.ref_order_no='保险')as costCheckOrder left join arap_cost_order acor on acor.id = costCheckOrder.cost_order_id
	left join user_login ul on ul.id = acor.create_by left join  user_login u on u.id = acor.confirm_by]]>
	</queryString>
