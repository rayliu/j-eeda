	<parameter name="order_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
  ifnull(amcoi.customer_order_no,'') customer_order_no,
  ifnull(amcoi.item_desc,'') item_desc,
  ifnull(amcoi.amount,'') amount,
  ifnull(fi. NAME,'') NAME
FROM
	arap_misc_charge_order_item amcoi
LEFT JOIN arap_misc_charge_order amco on amcoi.misc_order_id=amco.id
LEFT JOIN fin_item fi ON amcoi.fin_item_id = fi.id
where amcoi.misc_order_id=$P{order_id}]]>
	</queryString>
