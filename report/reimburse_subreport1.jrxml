	<queryString>
		<![CDATA[SELECT
  d.revocation_amount revocation_amount,
  IFNULL(d.remark,'') remark,
  f1. NAME item,
  f2. NAME parentItem
FROM
	reimbursement_order_fin_item d
LEFT JOIN fin_item f1 ON d.fin_item_id = f1.id
LEFT JOIN fin_item f2 ON f2.id = f1.parent_id
WHERE
	d.order_id = $P{order_id}]]>
	</queryString>
